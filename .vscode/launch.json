{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug dia2code on cardState.dia",
            "type": "cppdbg",
            "request": "launch",
            "program": "/app/bin/dia2code",
            "args": [
                // "--debug",
                // "8",
                // "-ns",
                // "client",
                // "-d",
                // "/app/src/client",
                // "-t",
                // "cpp",
                // "/app/src/client/../client.dia",
                "-d",
                "/app/src/client",
                "/app/src/cardsState.dia",
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}